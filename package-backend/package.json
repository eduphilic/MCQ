{
  "private": true,
  "name": "@join-uniform/backend",
  "version": "1.0.0",
  "scripts": {
    "start": "npm-run-all clean start:tsc",
    "start:tsc": "tsc-watch --onFirstSuccess \"cross-env NODE_ENV=development node ../dist/backend/main.dev.js\"",
    "build": "tsc",
    "clean": "rimraf ../dist/backend",
    "lint": "npm-run-all lint:*",
    "lint:tslint": "tslint -p ./tsconfig.json -t verbose",
    "lint:eslint": "eslint \"src/**\" --ext .tsx --ext .ts",
    "deploy": "npm-run-all clean build deploy:firebase",
    "deploy:firebase": "firebase deploy"
  },
  "dependencies": {
    "@firebase/app-types": "0.4.0",
    "@nestjs/common": "^6.2.0",
    "@nestjs/core": "^6.2.0",
    "@nestjs/platform-express": "^6.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "firebase-admin": "^7.3.0",
    "firebase-functions": "^2.3.1",
    "firebase-tools": "^6.9.2",
    "http-proxy-middleware": "^0.19.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "serve-static": "^1.14.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@join-uniform/tools": "1.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/http-proxy-middleware": "^0.19.2",
    "@types/node": "^12.0.1",
    "@types/request": "^2.48.1",
    "@types/serve-static": "^1.13.2",
    "@types/yup": "^0.26.13",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "tsc-watch": "^2.2.0",
    "typescript": "^3.5.0-dev.20190514"
  }
}
