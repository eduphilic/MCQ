{
  "functions": {
    "source": "dist"
  },
  "hosting": {
    "public": "dist/frontend/static",
    "rewrites": [
      {
        "source": "/_app",
        "function": "pages__app_js"
      },
      {
        "source": "/_document",
        "function": "pages__document_js"
      },
      {
        "source": "/_error",
        "function": "pages__error_js"
      },
      {
        "source": "/",
        "function": "pages_index_js"
      },
      {
        "source": "/signup",
        "function": "pages_signup_js"
      },
      {
        "source": "/api/**",
        "function": "api"
      }
    ]
  }
}
