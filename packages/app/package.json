    "@storybook/addons": "^3.4.6",
    "@storybook/react": "^3.4.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-runtime": "^6.26.0",
    "npm-run-all": "^4.1.2",
