    "lodash.debounce": "^4.0.8",
    "placeholder": "1.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.1.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.0.0-next.66cc7a90",
    "redux": "^4.0.0",
    "resize-observer-polyfill": "^1.5.0",
