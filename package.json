    "react-router-dom": "^4.2.2",
    "react-swipeable-views": "^0.12.16",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "resize-observer-polyfill": "^1.5.0",
    "styled-components": "3.4.5",
