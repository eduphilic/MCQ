    "lscache": "^1.1.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.1.2",
    "react-localization": "^1.0.10",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.0.0-next.66cc7a90",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "resize-observer-polyfill": "^1.5.0",
    "styled-components": "^3.3.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.0.0-beta.46",
    "@dump247/storybook-state": "^1.5.0",
    "@storybook/addon-actions": "^3.4.6",
    "@storybook/addon-info": "^3.4.6",
    "@storybook/addon-knobs": "^3.4.6",
    "@storybook/addon-links": "^3.4.6",
    "@storybook/addon-options": "^3.4.6",
    "@storybook/addon-viewport": "^3.4.6",
    "@storybook/addons": "^3.4.6",
    "@storybook/react": "^3.4.6",
    "@types/jest": "^23.0.0",
    "@types/lodash.debounce": "^4.0.3",
    "@types/lscache": "^1.0.29",
    "@types/prop-types": "^15.5.3",
    "@types/react-dom": "^16.0.5",
    "@types/react-helmet": "^5.0.6",
    "@types/react-redux": "^6.0.1",
    "@types/react-router-dom": "^4.2.7",
    "@types/storybook__addon-actions": "^3.0.3",
    "@types/storybook__addon-info": "^3.2.3",
    "@types/storybook__addon-knobs": "^3.3.1",
    "@types/storybook__addon-options": "^3.2.1",
    "@types/storybook__react": "^3.0.7",
    "babel-plugin-styled-components": "^1.5.1",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.4",
    "react-app-rewire-typescript-babel-preset": "^2.2.0",
    "react-app-rewired": "^2.0.0",
    "react-docgen-typescript-loader": "^2.1.0",
    "rimraf": "^2.6.2",
    "shx": "^0.2.2",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
