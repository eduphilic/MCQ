{
  "private": true,
  "name": "join-uniform",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:strothj/bhartitest.git",
  "author": "strothj <jason@jasons.io>",
  "pre-commit": [
    "lint",
    "type-check"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development npm-run-all clean start:webpack",
    "start:webpack": "webpack -w",
    "start:webworkers": "cpx dist/webworkers/*.js dist/frontend/static/js",
    "build": "cross-env NODE_ENV=production npm-run-all clean build:frontend build:backend",
    "build:frontend": "next build src/frontend",
    "build:backend": "webpack",
    "serve": "cross-env NODE_ENV=production npm-run-all build serve:firebase",
    "serve:firebase": "firebase serve --port 3000",
    "clean": "rimraf dist",
    "lint": "npm-run-all lint:*",
    "lint:tslint": "tslint -p ./tsconfig.json -t verbose",
    "lint:eslint": "eslint \"src/**\" --ext .tsx --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@firebase/app-types": "^0.3.6",
    "@material-ui/core": "4.0.0-alpha.4",
