{
  "private": true,
  "name": "bhartitest",
  "version": "1.0.0",
  "engines": {
    "node": ">= 8.10",
    "yarn": ">= 1.5.1"
  },
  "precommit": [
    "type-check"
  ],
  "browserslist": {
    "development": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "scripts": {
    "start": "react-app-rewired start --scripts-version @strothj/react-scripts",
    "build": "npm-run-all clean build:*",
    "build:cra": "react-app-rewired build --scripts-version @strothj/react-scripts",
    "build:mv": "shx mv build dist",
    "clean": "rimraf build dist",
    "test": "react-app-rewired test --scripts-version @strothj/react-scripts --env=jsdom",
    "storybook": "start-storybook -c .storybook -s public -p 9001",
    "storybook:build": "build-storybook -c .storybook -s public -o dist/storybook",
    "type-check": "tsc",
    "lint": "tslint -p ./"
  },
  "dependencies": {
