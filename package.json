    "@nestjs/platform-express": "^6.0.3",
    "cookie-session": "^2.0.0-beta.3",
    "csurf": "^1.9.0",
    "express": "^4.16.4",
    "firebase-admin": "~7.0.0",
    "firebase-functions": "2.2.0",
    "immer": "^2.1.4",
    "localforage": "^1.7.3",
    "next": "^8.0.3",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "typeface-montserrat": "^0.0.54",
    "universal-rxjs-ajax": "^2.0.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-typescript": "^7.3.3",
