{
  "private": true,
  "name": "join-uniform",
  "jest": {
    "transform": {
      "^.+\\.[tj]sx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "ts",
      "tsx"
    ]
  },
  "scripts": {
    "start": "rimraf build build-server && razzle start",
    "build": "rimraf build build-server && razzle build",
    "clean": "rimraf build build-server",
    "test": "razzle test --env=jsdom",
    "lint": "tslint -p ./",
    "analyze": "cross-env ANALYZE=1 yarn build",
    "firebase:start": "firebase serve",
    "firebase:deploy": "firebase deploy",
    "firebase:login": "firebase login",
    "graphql:generate": "ts-node --project tools/tsconfig.json tools/graphqlgen.ts"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "@reach/router": "^1.2.1",
    "apollo-boost": "^0.1.16",
    "apollo-cache-inmemory": "^1.3.5",
    "apollo-client": "^2.4.2",
    "apollo-link-http": "^1.5.5",
    "apollo-link-schema": "^1.1.1",
    "apollo-server-koa": "^2.1.0",
    "babel-runtime": "^6.26.0",
    "firebase-admin": "^6.0.0",
    "firebase-functions": "^2.0.5",
    "googleapis": "^34.0.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^4.0.2",
    "jss": "^9.8.7",
    "koa": "^2.5.3",
    "lz-string": "^1.4.4",
    "node-fetch": "^2.2.0",
    "razzle": "^2.4.0",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "react-jss": "^8.6.1",
    "react-localization": "^1.0.13",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "@google-cloud/functions-emulator": "^1.0.0-beta.5",
    "@playlyfe/gql": "^2.6.0",
    "@types/graphql": "^14.0.3",
    "@types/jest": "^23.3.5",
    "@types/koa": "^2.0.46",
    "@types/koa-static": "^4.0.0",
    "@types/lz-string": "^1.3.32",
    "@types/node-fetch": "^2.1.2",
    "@types/reach__router": "^1.2.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-jss": "^8.6.0",
    "@types/styled-components": "^4.0.1",
    "@types/webpack-env": "^1.13.6",
    "babel-plugin-graphql-tag": "^1.6.0",
    "babel-plugin-styled-components": "^1.8.0",
    "cross-env": "^5.2.0",
    "firebase-tools": "^5.1.1",
    "graphqlgen": "^0.2.2",
    "koa-static": "^5.0.0",
    "prettier": "^1.14.3",
    "razzle-plugin-typescript": "^2.4.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.3",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
