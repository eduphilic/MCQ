{
  "private": true,
  "name": "join-uniform",
  "sideEffects": [
    "*.css"
  ],
  "engines": {
    "node": "8"
  },
  "jest": {
    "transform": {
      "^.+\\.[tj]sx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "ts",
      "tsx"
    ]
  },
  "scripts": {
    "start": "rimraf build build-server && razzle start",
    "build": "rimraf build build-server && razzle build",
    "clean": "rimraf build build-server",
    "test": "razzle test --env=jsdom",
    "lint": "tslint -p ./",
    "analyze": "cross-env ANALYZE=1 yarn build",
    "firebase:start": "firebase serve",
    "firebase:deploy": "firebase deploy",
    "firebase:login": "firebase login",
    "graphql": "gql-gen --schema src/api/schema/**/*.graphql --template graphql-codegen-typescript-resolvers-template --out src/api/generated.ts",
    "graphql:watch": "yarn graphql --watch"
  },
  "dependencies": {
    "@firebase/app-types": "^0.3.2",
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "@reach/router": "^1.2.1",
    "apollo-cache-inmemory": "^1.3.8",
    "apollo-client": "^2.4.5",
    "apollo-link": "^1.2.3",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "apollo-link-schema": "^1.1.1",
    "apollo-link-state": "^0.4.2",
    "apollo-server-koa": "2.2.0-alpha.1",
    "babel-runtime": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "firebase-admin": "^6.1.0",
    "firebase-functions": "^2.1.0",
    "formik": "^1.3.1",
    "google-auth-library": "^2.0.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^4.0.3",
    "jsonwebtoken": "^8.3.0",
    "jss": "^9.8.7",
    "koa": "^2.6.1",
    "koa-csrf": "^3.0.6",
    "koa-session": "^5.10.0",
    "lodash.merge": "^4.6.1",
    "lz-string": "^1.4.4",
    "node-fetch": "^2.2.0",
    "razzle": "^2.4.0",
    "react": "16.7.0-alpha.0",
    "react-apollo": "2.3.0",
    "react-dom": "16.7.0-alpha.0",
    "react-jss": "^8.6.1",
    "react-loadable": "^5.5.0",
    "react-localization": "^1.0.13",
    "styled-components": "^4.0.3",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@google-cloud/functions-emulator": "^1.0.0-beta.5",
    "@playlyfe/gql": "^2.6.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/graphql": "^14.0.3",
    "@types/jest": "^23.3.9",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/koa": "^2.0.46",
    "@types/koa-csrf": "^3.0.1",
    "@types/koa-session": "^5.7.4",
    "@types/koa-static": "^4.0.0",
    "@types/lodash.merge": "^4.6.4",
    "@types/lz-string": "^1.3.32",
    "@types/node-fetch": "^2.1.2",
    "@types/reach__router": "^1.2.1",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-jss": "^8.6.0",
    "@types/react-loadable": "^5.4.1",
    "@types/styled-components": "^4.0.3",
    "@types/webpack-env": "^1.13.6",
    "@types/yup": "^0.26.1",
    "babel-plugin-graphql-tag": "^1.6.0",
    "babel-plugin-styled-components": "^1.8.0",
    "cross-env": "^5.2.0",
    "firebase-tools": "^6.0.1",
    "graphql-code-generator": "^0.13.0",
    "graphql-codegen-typescript-resolvers-template": "^0.13.0",
    "koa-static": "^5.0.0",
    "prettier": "^1.14.3",
    "razzle-plugin-typescript": "^2.4.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.1.6",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-shell-plugin": "^0.5.0"
  }
}
