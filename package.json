{
  "private": true,
  "name": "bhartitest",
  "version": "1.0.0",
  "engines": {
    "node": ">= 8.10",
    "yarn": ">= 1.5.1"
  },
  "precommit": [
    "lint",
    "type-check"
  ],
  "browserslist": {
    "development": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "npm-run-all clean build:* storybook:build",
    "build:cra": "react-app-rewired build",
    "build:mv": "shx mv build dist",
    "clean": "rimraf build dist",
    "test": "react-app-rewired test --env=jsdom",
    "storybook": "start-storybook -c .storybook -s public -p 9001",
    "storybook:build": "build-storybook -c .storybook -s public -o dist/storybook",
    "type-check": "tsc",
    "lint": "tslint -p ./"
  },
  "dependencies": {
    "@material-ui/core": "^1.1.0",
    "@material-ui/icons": "^1.1.0",
    "downshift": "^1.31.15",
    "formik": "^0.11.11",
